{
  "name": "nojvek.com",
  "version": "1.0.0",
  "private": true,
  "author": "nojvek",
  "homepage": "http://nojvek.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nojvek/nojvek.com.git"
  },
  "scripts": {
    "build": "npm run pug && npm run styl",
    "pug": "node node_modules/pug-cli --out docs src",
    "styl": "node node_modules/stylus/bin/stylus --inline --sourcemap --compress --out docs src",
    "server": "cd docs && node ../node_modules/http-server/bin/http-server"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.11.1",
    "pug-cli": "^1.0.0-alpha6",
    "stylus": "^0.54.5"
  }
}
